<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.onlylemi.mapview"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:name="kz.mobdev.mapview.DemoApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="kz.mobdev.mapview.MainActivity"
            android:theme="@style/AppTheme.Boot">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="kz.mobdev.mapview.MapLayerTestActivity"
            android:label="MapLayerTest">
        </activity>
        <activity
            android:name="kz.mobdev.mapview.LocationLayerTestActivity"
            android:label="LocationLayerTest">
        </activity>
        <activity
            android:name="kz.mobdev.mapview.BitmapLayerTestActivity"
            android:label="BitmapLayerTest">
        </activity>
        <activity
            android:name="kz.mobdev.mapview.MarkLayerTestActivity"
            android:label="MarkLayerTest">
        </activity>
        <activity
            android:name="kz.mobdev.mapview.RouteLayerTestActivity"
            android:label="RouteLayerTest">
        </activity>
    </application>

</manifest>